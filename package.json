{
  "name": "transwarp",
  "version": "0.0.1",
  "description": "ORM for Node.js",
  "author": "Fangdun Cai <cfddream@gmail.com>",
  "license": "MIT",
  "main": "src/index",
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-core/register --bail --check-leaks test",
    "test-cov": "babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha -- -u exports --bail --check-leaks test",
    "test-travis": "babel-node node_modules/.bin/isparta cover --report lcovonly node_modules/.bin/_mocha -- -u exports --bail --check-leaks test"
  },
  "dependencies": {
    "babel-core": "^6.3.2",
    "koa-compose": "^3.0.0",
    "lodash": "^4",
    "pluralize": "^1.2.1",
    "validator": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-es2015-destructuring": "^6.1.18",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.x",
    "babel-plugin-transform-es2015-parameters": "^6.1.18",
    "babel-preset-stage-1": "^6.x",
    "babel-preset-stage-2": "^6.1.18",
    "babel-preset-stage-3": "^6.x",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "isparta": "^4.x",
    "mocha": "^2.x",
    "mongodb": "^2.x",
    "mysql": "^2.x",
    "pg": "^4.x",
    "sqlite3": "^3.x"
  },
  "babel": {
    "plugins": [
      "transform-es2015-destructuring",
      "transform-es2015-modules-commonjs",
      "transform-es2015-parameters"
    ],
    "presets": [
      "stage-1",
      "stage-2",
      "stage-3"
    ]
  }
}
